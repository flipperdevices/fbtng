{
    "cpu_flags": [
        "-mcpu=cortex-m4",
        "-mthumb",
        "-mfloat-abi=hard",
        "-mfpu=fpv4-sp-d16"
    ],
    "c_cpp_flags": [
        "-g",
        "-Wall",
        "-Wextra",
        "-Werror",
        "-Wundef",
        "-Wno-error=deprecated-declarations",
        "-Wno-address-of-packed-member",
        "-Wredundant-decls",
        "-Wdouble-promotion",
        "-fdata-sections",
        "-ffunction-sections",
        "-fsingle-precision-constant",
        "-fno-math-errno"
    ],
    "c_flags": [
        "-std=gnu2x",
        "-Wstrict-prototypes"
    ],
    "cpp_flags": [
        "-std=c++20",
        "-fno-rtti",
        "-fno-use-cxa-atexit",
        "-fno-exceptions",
        "-fno-threadsafe-statics",
        "-ftemplate-depth=4096"
    ],
    "asm_flags": [
        "-x",
        "assembler-with-cpp",
        "-c"
    ],
    "linker_flags": [
        "-mlittle-endian",
        "-specs=nano.specs",
        "-Wl,--gc-sections",
        "-Wl,--no-warn-rwx-segment",
        "-Wl,--wrap,_malloc_r",
        "-Wl,--wrap,_free_r",
        "-Wl,--wrap,_calloc_r",
        "-Wl,--wrap,_realloc_r"
    ],
    "defines": [
        "_GNU_SOURCE"
    ],
    "include_paths": [
        "../../lib/stm32wb_copro/wpan/interface/patterns/ble_thread/tl",
        "ble_glue",
        "fatfs",
        "furi_hal",
        "inc"
    ],
    "sdk_headers": [
        "../furi_hal_include/*.h",
        "!furi_hal/*_i.h",
        "furi_hal/furi_hal_mpu.h",
        "furi_hal/furi_hal_pwm.h",
        "furi_hal/furi_hal_usb_cdc.h",
        "furi_hal/furi_hal_usb_hid_u2f.h",
        "platform_specific/*.h",
        "ble_glue/*/*.h"
    ],
    "sources": [
        "*/*.c"
    ],
    "linker_script_flash": "stm32wb55xx_flash.ld",
    "linker_script_ram": "stm32wb55xx_ram_fw.ld",
    "linker_script_app": "application_ext.ld",
    "sdk_symbols": "api_symbols.csv",
    "rtos_flavor": "freertos-cm4",
    "variables_sconscript": "fbt_conf/stm32wb_vars.scons",
    "lib_modules": [
        "fw_updater_opts",
        "lib_base",
        "wb55_lib_config",
        "mlib",
        "cmsis",
        "stm32wb",
        "freertos",
        "libusb_stm32",
        "drivers",
        "microtar",
        "mbedtls",
        "icon_assets",
        "toolbox",
        "print",
        "fatfs",
        "flipper_format",
        "one_wire",
        "ibutton",
        "infrared",
        "subghz",
        "nfc",
        "digital_signal",
        "pulse_reader",
        "signal_reader",
        "u8g2",
        "canvas_u8g2",
        "lfrfid",
        "flipper_application",
        "music_worker",
        "mjs",
        "nanopb",
        "update_util",
        "heatshrink",
        "ble_profile",
        "bit_lib",
        "datetime",
        "dolphin",
        "rpc_proto",
        "ext_resources",
        "targets",
        "version",
        "furi",
        "ext_applications",
        "firmware_applications",
        "elf_mapfile_gen"
    ],
    "fw_modules": [
        "app_check",
        "fw_elf",
        "fw_artifacts",
        "sdk_extapps",
        "resources",
        "fw_cdb",
        "static_analysis",
        "link_builddir"
    ],
    "dist_modules": [
        "elf_flash",
        "debug",
        "fap_deploy",
        "wb55_dist",
        "lint",
        "devboard",
        "doxygen",
        "vscode_env",
        "cli"
    ],
    "platform": "stm32wb",
    "svd_file": "STM32WB55_CM4.svd",
    "flash_address": "0x08000000"
}
