{
    "cpu_flags": [
        "-mcpu=cortex-m33",
        "-mthumb",
        "-mfloat-abi=hard",
        "-mfpu=fpv5-sp-d16"
    ],
    "c_cpp_flags": [
        "-g",
        "-Wall",
        "-Wextra",
        "-Werror",
        "-Wundef",
        "-Wno-error=deprecated-declarations",
        "-Wno-address-of-packed-member",
        "-Wredundant-decls",
        "-Wdouble-promotion",
        "-fdata-sections",
        "-ffunction-sections",
        "-fsingle-precision-constant",
        "-fno-math-errno"
    ],
    "c_flags": [
        "-std=gnu2x",
        "-Wstrict-prototypes"
    ],
    "cpp_flags": [
        "-std=c++20",
        "-fno-rtti",
        "-fno-use-cxa-atexit",
        "-fno-exceptions",
        "-fno-threadsafe-statics",
        "-ftemplate-depth=4096"
    ],
    "asm_flags": [
        "-x",
        "assembler-with-cpp",
        "-c"
    ],
    "linker_flags": [
        "-specs=nano.specs",
        "-mlittle-endian",
        "-Wl,--gc-sections",
        "-Wl,--no-warn-rwx-segment",
        "-Wl,--wrap,_malloc_r",
        "-Wl,--wrap,_free_r",
        "-Wl,--wrap,_calloc_r",
        "-Wl,--wrap,_realloc_r"
    ],
    "defines": [
        "STM32U5G9xx",
        [
            "HSE_VALUE",
            "16000000U"
        ],
        "_GNU_SOURCE"
    ],
    "include_paths": [
        "src",
        "config",
        "fatfs",
        "furi_hal",
        "st",
        "st/include",
        "driver",
        "st_abomination"
    ],
    "sdk_header_paths": [],
    "startup_script": "startup_stm32u5g9xx.s",
    "linker_script_flash": "STM32U5g9xx_FLASH.ld",
    "linker_script_ram": "STM32U5g9xx_FLASH.ld",
    "linker_script_app": "application_ext.ld",
    "sdk_symbols": "api_symbols.csv",
    "rtos_flavor": "freertos-cm33-ntz",
    "env_modules": [
        "fw_updater_opts",
        "lib_base",
        "mlib",
        "cmsis",
        "freertos",
        "microtar",
        "mbedtls",
        "icon_assets",
        "toolbox",
        "tinyusb",
        "print",
        "fatfs",
        "flipper_format",
        "nema_gfx",
        "canvas_nema",
        "flipper_application",
        "nanopb",
        "update_util",
        "heatshrink",
        "bit_lib",
        "datetime",
        "targets",
        "furi",
        "ext_applications",
        "applications"
    ],
    "fw_modules": [
        "fw_elf",
        "fw_artifacts",
        "fw_cdb",
        "link_builddir"
    ],
    "dist_modules": [
        "elf_flash",
        "debug",
        "vscode_env",
        "cli"
    ],
    "platform": "stm32u5",
    "svd_file": "STM32U5Gx.svd",
    "flash_address": "0x08000000"
}