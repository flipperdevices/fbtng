{
    "cpu_flags": [
        "-mcpu=cortex-m4",
        "-mthumb",
        "-mfloat-abi=hard",
        "-mfpu=fpv4-sp-d16"
    ],
    "c_cpp_flags": [
        "-g",
        "-Wall",
        "-Wextra",
        "-Werror",
        "-Wundef",
        "-Wno-error=deprecated-declarations",
        "-Wno-address-of-packed-member",
        "-Wredundant-decls",
        "-Wdouble-promotion",
        "-fdata-sections",
        "-ffunction-sections",
        "-fsingle-precision-constant",
        "-fno-math-errno"
    ],
    "c_flags": [
        "-std=gnu2x",
        "-Wstrict-prototypes"
    ],
    "cpp_flags": [
        "-std=c++20",
        "-fno-rtti",
        "-fno-use-cxa-atexit",
        "-fno-exceptions",
        "-fno-threadsafe-statics",
        "-ftemplate-depth=4096"
    ],
    "asm_flags": [
        "-x",
        "assembler-with-cpp",
        "-c"
    ],
    "linker_flags": [
        "-specs=nano.specs",
        "-mlittle-endian",
        "-Wl,--gc-sections",
        "-Wl,--no-warn-rwx-segment",
        "-Wl,--wrap,_malloc_r",
        "-Wl,--wrap,_free_r",
        "-Wl,--wrap,_calloc_r",
        "-Wl,--wrap,_realloc_r"
    ],
    "defines": [
        "_GNU_SOURCE",
        "SI917",
        "SLI_SI917",
        "SLI_SI917B0",
        "SIWG917M111MGTBA",
        "USER_CONFIGURATION_ENABLE",
        "SLI_SI91X_MCU_CONFIG_RADIO_BOARD_BASE_VER",
        "SLI_SI91X_MCU_CONFIG_RADIO_BOARD_VER2"
    ],
    "include_paths": [
        "config"
    ],
    "sdk_header_paths": [
    ],
    "linker_script_flash": "SiWG917M111_flash.ld",
    "rtos_flavor": "freertos-cm4",
    "env_modules": [
        "cmsis",
        "wiseconnect",
        "targets"
    ],
    "platform": "si917",
    "svd_file": "SiWG917_CM4.svd",
    "flash_address": "0x08202000"
}
