    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach FW (ST-Link)",
            "cwd": "${workspaceFolder}",
            "executable": "./build/latest/firmware.elf",
            "request": "attach",
            "type": "cortex-debug",
            "servertype": "openocd",
            "device": "stlink",
            "svdFile": "./scripts/debug/STM32U5Gx.svd",
            "rtos": "FreeRTOS",
            "configFiles": [
                "interface/stlink-dap.cfg",
                "./scripts/debug/stm32u5x.cfg"
            ],
            "postAttachCommands": [
                // "source scripts/debug/flipperversion.py",
                // "fw-version",
                "source scripts/debug/flipperapps.py",
                "fap-set-debug-elf-root build/latest/.extapps"
            ],
            // "showDevDebugOutput": "raw",
        },
